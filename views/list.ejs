<h1 class="display-4">Usuários</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>#</th>
      <th>Título</th>
      <th>Idade</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach(function(user) { %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= user.age %></td>
        <td>
          <a href="/user/edit/<%= user.id %>" class="btn btn-primary btn-sm">Editar</a>
          <form action="/user/delete/<%= user.id %>" method="post" class="d-inline">
            <button class="btn btn-danger btn-sm">Deletar</button>
          </form>
        </td>
      </tr>
    <% }) %>
  </tbody>
</table>
